@model PDFOnlineSignature.Models.Document
@{
    ViewData["Title"] = "Document Signed - "+Model.Name;
}
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h3 class="panel-title">Your document @Model.Name has been signed</h3>
            </div>
            <div class="panel-body">
                <p>We have sent your document to your email address, please check your inbox.</p>
                <p>If you need further assistance please contact your system administrator.</p>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <p class="navbar-text"><button type="button" class="btn btn-primary" onclick="location.href='@Url.Action("Index", "Home")'">Ok</button></p>
            </div>
        </nav>
    </div>
</div>